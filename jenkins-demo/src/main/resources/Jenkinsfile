pipeline{
    agent any
    stages{
        stage('Build MVN'){
            steps{
                echo 'Build Maven Project'
                sh 'cd jenkins-demo && mvn -B -DskipTests clean package'
            }
        }
        stage('Test'){
            steps{
                echo 'Testing..'
                cd 'jenkins-demo && mvn test'
            }
        }
        stage('Deploy'){
            steps{
                echo 'Deploy Project'
            }
        }
    }
}